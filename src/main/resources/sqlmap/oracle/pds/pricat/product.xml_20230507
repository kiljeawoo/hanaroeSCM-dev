			, A.NA_WRS_PAK_C 	/* 경제통합상품포장코드 */
			, FN_CM_SIMP_CNM('NA_WRS_PAK_C', A.NA_WRS_PAK_C) AS NA_WRS_PAK_NM	/* 경제통합상품포장명 */
			, A.NA_WRS_IPO_DSC /* 경제통합상품수입구분코드 */
			, A.RFID_UYN /* RFID사용여부 */
			, A.NA_WRS_CSTD_METC	/* 경제통합상품보관방식코드 */
			, FN_CM_SIMP_CNM('NA_WRS_CSTD_METC', A.NA_WRS_CSTD_METC) AS NA_WRS_CSTD_METC_NM	/* 경제통합상품보관방식명 */
			, FN_CM_SIMP_CNM('NA_WRS_UZC', A.NA_WRS_UZC) AS NA_WRS_UZC	/* 경제통합상품용도코드 */
			, A.DSTR_TER_YN /* 유통기한관리여부 */
			, A.DSTR_TER_MRK_DSC /* 유통기한표기방법구분코드 */
			, FN_CM_SIMP_CNM('DSTR_TER_MRK_DSC', A.DSTR_TER_MRK_DSC) AS DSTR_TER_MRK_DSC_NM /* 유통기한표기방법구분코드명 */
			, A.NA_WRS_PROENV_DSC	/* 친환경인증 */
			, FN_CM_SIMP_CNM('NA_WRS_PROENV_DSC', A.NA_WRS_PROENV_DSC) AS NA_WRS_PROENV_DSC_NM  /*친환경인증구분명*/
			, A.BOXPE_AQZ	/*박스당입수*/
			, A.LIQ_UZ_DSC /* 주류용도 */
			, FN_CM_SIMP_CNM('LIQ_UZ_DSC', A.LIQ_UZ_DSC) AS LIQ_UZ_DSC_NM /* 주류용도명 */
			, A.NA_WRS_QLT_GRDC /* 품질(등급) */
			, FN_CM_SIMP_CNM('NA_WRS_QLT_GRDC', A.NA_WRS_QLT_GRDC) AS NA_WRS_QLT_GRDC_NM /* 품질(등급)명 */
			, A.NA_WRS_SZE_SEX_C /* 크기(성별) */
			, FN_CM_SIMP_CNM('NA_WRS_SZE_SEX_C', A.NA_WRS_SZE_SEX_C) AS NA_WRS_SZE_SEX_C_NM /* 크기(성별)명 */
			, A.NA_WRS_DSTR_STSC	/*경제통합상품유통상태코드*/
			, FN_CM_SIMP_CNM('NA_WRS_DSTR_STSC', A.NA_WRS_DSTR_STSC) AS NA_WRS_DSTR_STSC_NM	/*경제통합상품유통상태명*/
			, A.VLD_PRD_CN /* 유효기간 */
			, A.VLD_PRD_DSC	/*유효기간구분코드*/
			, FN_CM_SIMP_CNM('VLD_PRD_DSC', A.VLD_PRD_DSC) AS VLD_PRD_DSC_NM	/*유효기간구분명*/
			, A.BSC_UNT_DSC	/*기본단위구분코드*/
			, FN_CM_SIMP_CNM('BSC_UNT_DSC', A.BSC_UNT_DSC) AS BSC_UNT_DSC_NM	/*기본단위구분명*/
			, A.PR_MRK_WHT
			, A.PR_MRK_STD_BAS_WHT
			, A.PR_MRK_BAS_UNIT
			, FN_CM_SIMP_CNM('BAS_PR_UNT_DSC', A.PR_MRK_BAS_UNIT) AS PR_MRK_BAS_UNIT_NM
		FROM
			TB_GD_WRS_RG_REQ A, 			/* 상품등록신청내역 */
			TB_GD_WRS_CLF B
		WHERE 1=1
			AND A.NA_WRS_LCLC =B.NA_WRS_LCLC
			AND A.NA_WRS_MCLC = B.NA_WRS_MCLC
			AND A.NA_WRS_SCLC = B.NA_WRS_SCLC
			AND A.NA_WRS_DTCF_C = B.NA_WRS_DTCF_C
			AND A.FSRGMN_NA_BZPLC = #{FSRGMN_NA_BZPLC}
			AND A.VAN_C_RQ_NO = #{VAN_C_RQ_NO}
			AND A.CUD_DSC != 'D'
	</select>
	
