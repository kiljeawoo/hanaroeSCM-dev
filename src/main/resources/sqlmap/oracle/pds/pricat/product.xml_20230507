			, A.CNF_NAME /*승인/거절자명*/
			, A.NA_C_RQ_REJ_RSNC /*경제통합코드신청거절사유코드*/
			, FN_CM_SIMP_CNM('NA_C_RQ_REJ_RSNC', A.NA_C_RQ_REJ_RSNC) AS NA_C_RQ_REJ_RSNC_NM /*경제통합코드신청거절사유코드명*/
			, A.NA_C_RQ_STSC /*경제통합코드신청상태코드*/
			, FN_CM_SIMP_CNM('NA_C_RQ_STSC', A.NA_C_RQ_STSC) AS NA_C_RQ_STSC_NM /*경제통합코드신청상태코드명*/
			, TO_CHAR(A.PR_MRK_WHT) AS PR_MRK_WHT
			, A.PR_MRK_STD_BAS_WHT
			, TO_CHAR(A.PR_MRK_BAS_UNIT) AS PR_MRK_BAS_UNIT
			, FN_CM_SIMP_CNM('BAS_PR_UNT_DSC', A.PR_MRK_BAS_UNIT) AS PR_MRK_BAS_UNIT_NM
			, A.FSRG_DTM
			/* 새로추가 */
			, LG.NA_SPYPL_C /*경제통합공급처코드*/
			, LG.NA_SPYPL_TEAM_C /*거래처팀코드*/
			, LG.BOX_WRS_YN /*박스상품여부*/
			, LG.BOX_WT /*박스중량*/
			, LG.BOX_BULK /*박스용적*/
			, LG.BYNG_UPR /*매입단가*/
			, LG.SEL_UPR /*판매단가*/
			, LG.WDTH_LEN /*가로길이*/
			, LG.LEN_LEN /*세로길이*/
			, LG.WRS_HGHT /*상품높이*/
			, LG.PLTPE_LYNO /*파렛트당층수*/
			, LG.PLTPE_SGNO /*파렛트당단수*/
			, LG.PLTPE_BOX_CN /*파렛트당박스수*/
		FROM 
			TB_GD_WRS_RG_REQ A,
			TB_GD_WRS_RG_REQ_LG LG
		WHERE 1=1
			<if test="TRPL_C != null and TRPL_C.length > 0">
				AND A.FSRGMN_NA_BZPLC IN
				<foreach item="item" collection="TRPL_C" open="(" close=")"
					separator=",">
					#{item}
				</foreach>
			</if>
			<if test="CBO_RESDSC != null and CBO_RESDSC != 'all'">
				AND A.NA_C_RQ_STSC = #{CBO_RESDSC}
			</if>
			<if
				test="CBO_WRSDSC != null and CBO_WRSDSC.length() > 0 and EDT_WRS != null and EDT_WRS.length() > 0">
				<choose>
					<when test="CBO_WRSDSC == 'CODE'.toString()">
						AND A.RQ_NA_WRS_C = #{EDT_WRS}
					</when>
					<when test="CBO_WRSDSC == 'NAME'.toString()">
						AND A.WRSNM LIKE '%' || #{EDT_WRS} || '%'
					</when>
				</choose>
			</if>
			<if
				test="DT_STDT != null and DT_STDT.length() > 0 and DT_EDDT != null and DT_EDDT.length() > 0">
				AND TO_CHAR(A.FSRG_DTM, 'YYYYMMDD') BETWEEN #{DT_STDT} AND #{DT_EDDT}
			</if>
			<if
				test="CBO_BUYERDSC != null and CBO_BUYERDSC != 'all' and EDT_BUYER != null and EDT_BUYER.length() > 0">
				<choose>
					<when test="CBO_BUYERDSC == 'ENO'.toString()">
						AND TRIM(A.CNF_ENO) = #{EDT_BUYER}
					</when>
					<when test="CBO_BUYERDSC == 'NAME'.toString()">
						AND A.CNF_NAME = #{EDT_BUYER}
					</when>
				</choose>
			</if>
			AND A.CUD_DSC != 'D'
			AND A.VAN_C_RQ_NO = LG.VAN_C_RQ_NO
		ORDER BY FSRG_DTM
	</select>
	
	<!--  차세대 이전 쿼리 -->
	<select id="retrieveNewGoodsDetailByKornet_asis" parameterType="com.nh.escm.pds.pricat.vo.ProductReqVO" resultType="java.util.Map">
		SELECT
			<!-- 공통정보 -->
