	<select id="selectOnGdWrsNtfyDtl" parameterType="java.util.Map" resultType="java.util.Map">
		SELECT ROWNUM   AS SORT_SEQ  /* 정렬순서   */
		     , A.WRS_NTFY_C          /* 고시코드   */
