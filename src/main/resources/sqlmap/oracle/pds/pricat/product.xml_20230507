	<sql id="sql_selectModGoodsByKornet">
		SELECT
			 A.VAN_C_RQ_NO
			,A.NA_C_RQ_NO
			,A.APV_RQ_NA_BZPLC
			,CASE WHEN A.CUD_DSC = 'I' THEN '신청'
				WHEN A.CUD_DSC = 'U' THEN '수정'
				ELSE ''
			END AS GBN
			,CASE WHEN A.PRC_YN IN ('Y', 'R') THEN '정상'
				 WHEN A.PRC_YN IS NULL THEN ''
				 WHEN A.PRC_YN IN ('N', 'P') THEN '전송중'
				 ELSE '오류'
		 	 END RES_CODE
			,A.RQ_NA_WRS_C
			,A.WRSNM
			,'' AS REQ_DT
			,FN_TRPL_CIF_NM(A.FSRGMN_NA_BZPLC) AS FSRGMN_NA_BZPL_NM
			,A.FSRGMN_ENO
			,'' AS BCD_NO
			,A.CNF_DMT AS RES_DT
			,A.CNF_ENO AS BUYER_ENO
			,A.CNF_NAME AS BUYER_NM
			,A.REQ_RESULT
			,'' AS ETC
			, (SELECT COUNT(*) FROM TB_GD_WRS_RG_REQ_FILE B WHERE B.VAN_C_RQ_NO = A.VAN_C_RQ_NO AND B.FILE_SEQ = 1) AS IMG1
			, (SELECT COUNT(*) FROM TB_GD_WRS_RG_REQ_FILE B WHERE B.VAN_C_RQ_NO = A.VAN_C_RQ_NO AND B.FILE_SEQ = 2) AS IMG2
			, (SELECT COUNT(*) FROM TB_GD_WRS_RG_REQ_FILE B WHERE B.VAN_C_RQ_NO = A.VAN_C_RQ_NO AND B.FILE_SEQ = 3) AS IMG3
			, (SELECT COUNT(*) FROM TB_GD_WRS_RG_REQ_FILE B WHERE B.VAN_C_RQ_NO = A.VAN_C_RQ_NO AND B.FILE_SEQ = 4) AS IMG4
		FROM TB_GD_WRS_RG_MOD_REQ A, TB_TR_TRPL_CIF C
		WHERE
			A.FSRGMN_NA_BZPLC = C.NA_TRPL_C
			<if test="TRPL_C != null and TRPL_C.length > 0">
				AND A.FSRGMN_NA_BZPLC IN
				<foreach item="item" collection="TRPL_C" open="(" close=")" separator=",">
					#{item}
				</foreach>
			</if>
			<if test="DT_STDT != null and DT_STDT.length() > 0 and DT_EDDT != null and DT_EDDT.length() > 0">

			</if>
			<if test="CBO_RESDSC != null and CBO_RESDSC != 'all'">
					AND A.NA_C_RQ_STSC = #{CBO_RESDSC}
			</if>
			<if test="CBO_WRSDSC != null and CBO_WRSDSC.length() > 0 and EDT_WRS != null and EDT_WRS.length() > 0">
				<choose>
					<when test="CBO_WRSDSC == 'CODE'.toString()">
						AND A.RQ_NA_WRS_C = #{EDT_WRS}
					</when>
					<when test="CBO_WRSDSC == 'NAME'.toString()">
						AND A.WRSNM LIKE '%' || #{EDT_WRS} || '%'
					</when>
				</choose>
			</if>
			<if test="CBO_CORPDSC != null and CBO_CORPDSC != 'all' and EDT_CORP != null and EDT_CORP.length() > 0">
				<choose>
					<when test="CBO_CORPDSC == 'GLN'.toString()">
						AND A.FSRGMN_NA_BZPLC = #{EDT_CORP}
					</when>
					<when test="CBO_CORPDSC == 'NAME'.toString()">
						AND C.CLNTNM LIKE '%' || #{EDT_CORP} || '%'
					</when>
					<when test="CBO_CORPDSC == 'CORP'.toString()">
						AND C.BIZ_NUMBER = #{EDT_CORP}
					</when>
				</choose>
			</if>
			<if test="CBO_BUYERDSC != null and CBO_BUYERDSC != 'all' and EDT_BUYER != null and EDT_BUYER.length() > 0">
				<choose>
					<when test="CBO_BUYERDSC == 'ENO'.toString()">
						AND A.CNF_ENO = #{EDT_BUYER}
					</when>
					<when test="CBO_BUYERDSC == 'NAME'.toString()">
						AND A.CNF_NAME = #{EDT_BUYER}
					</when>
				</choose>
			</if>
			AND A.CUD_DSC != 'D'
	</sql>

