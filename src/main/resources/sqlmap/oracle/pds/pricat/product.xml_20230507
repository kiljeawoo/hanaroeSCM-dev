	<!-- 상품정보 신청 내역 조회-->
	<select id="retrieveWRSReqExist" parameterType="java.util.Map" resultType="java.util.Map">
		SELECT A.*
		FROM
			TB_GD_WRS_RG_REQ A
		WHERE A.RQ_NA_WRS_C = #{NA_WRS_C}
		  AND ( A.NA_C_RQ_STSC NOT IN ('3', '4') /* 3:승인, 4:반려 */
				OR A.NA_C_RQ_STSC IS NULL
