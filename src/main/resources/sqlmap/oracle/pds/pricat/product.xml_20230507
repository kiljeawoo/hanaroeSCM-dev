			,A.VCBT_NA_WRS_C
			,A.VCBT_QT
			,A.VCBX_NA_WRS_C
			,A.WRS_DTL_EXPL
			,A.BSC_UNT_DSC
			,A.BOXPE_AQZ
			,A.RFID_UYN
			,A.FOMNM
			,A.SLGT_ETR_DSC
			,A.DSTR_TER_YN
			,A.DSTR_TER_MRK_DSC
			,A.WRS_MODIFY_YN
			,A.TEMP_YN
			,A.PRC_YN
			,A.RSP_C_CNTN
			,A.REQ_RESULT
			,A.CNF_ENO
			,A.CNF_DMT
			,A.CNF_NAME
			,A.NA_C_RQ_REJ_RSNC
			,A.NA_C_RQ_STSC
			,C.BOXPE_DLVG_UPR
			,C.SLAM_PER_FEERT
			,C.PMTN_SPT_PERS
			,C.WRS_TRT_CMBR_CN
			,C.ASP_SL_CNAM
		FROM
			TB_GD_WRS_RG_REQ A
			<choose>
				<when test="GUBUN == T">
					,TB_GD_WRS B
				</when>
				<otherwise>
					,(SELECT NA_WRS_C FROM TB_GD_WRS WHERE NA_WRS_C = #{NA_WRS_C}) B
				</otherwise>
			</choose>
			,TB_GD_WRS_BS_CM_RG_RQ C
		WHERE
			<choose>
				<when test="GUBUN == T">
					A.RQ_NA_WRS_C = B.NA_WRS_C(+)
					AND A.NA_C_RQ_NO = C.NA_C_RQ_NO(+)
				</when>
				<otherwise>
					B.NA_WRS_C IS NULL
					AND A.NA_C_RQ_NO = C.NA_C_RQ_NO
					AND A.RQ_NA_WRS_C = B.NA_WRS_C(+)
				</otherwise>
			</choose>
			AND A.PRC_YN = 'Y'

			AND A.FSRGMN_NA_BZPLC IN
		  	<foreach collection="TRPL_C" item="item" index="index" separator="," open="(" close=")">
	    		#{item}
			</foreach>
	</select>

