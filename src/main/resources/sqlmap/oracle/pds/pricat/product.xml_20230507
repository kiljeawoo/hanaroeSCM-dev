	    		TB_TR_TRPL_CIF B
			WHERE
	  			A.SPYPL_NA_TRPL_C = #{NA_BZPLC}
	  			AND A.SPYPL_NA_TRPL_C = B.NA_TRPL_C
	  			AND A.DEL_DTM IS NULL
	  			AND A.WRSNM IS NOT NULL
			GROUP BY
				A.SPYPL_NA_TRPL_C,
				A.NA_WRS_C,
				A.WRSNM,
				B.CLNTNM
			ORDER BY
				A.SPYPL_NA_TRPL_C,
				A.NA_WRS_C
