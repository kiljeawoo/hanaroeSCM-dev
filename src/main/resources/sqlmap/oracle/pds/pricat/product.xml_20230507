	<sql id = "sql_selectMDTempOdTrtwrsList">
	    SELECT
			MD_NM,
			SPYPL_NA_TRPL_C,
			SPYPL_NA_TRPL_NM,
			COUNT(SPYPL_NA_TRPL_C) AS NA_WRS_C_CNT
		FROM
			NHVANINF.TEMP_OD_TRTWRS_INFO
		<where>
			<if test="MD_NM != null and MD_NM.length() > 0" >
			   	AND MD_NM LIKE '%' || #{MD_NM} || '%'
			</if>
			<if test="SPYPL_NA_TRPL_C != null and SPYPL_NA_TRPL_C > 0" >
			   	AND SPYPL_NA_TRPL_C = #{SPYPL_NA_TRPL_C}
			</if>
		</where>
		GROUP BY
			SPYPL_NA_TRPL_C,
			SPYPL_NA_TRPL_NM,
			MD_NM
		ORDER BY
			SPYPL_NA_TRPL_C
	</sql>

