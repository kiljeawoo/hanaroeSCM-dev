	      , A.NA_WRS_DSTR_STSC  /*경제통합상품유통상태코드*/
	      , FN_CM_SIMP_CNM('NA_WRS_DSTR_STSC', A.NA_WRS_DSTR_STSC) AS NA_WRS_DSTR_STSC_NM  /*경제통합상품유통상태명*/
	      , A.VLD_PRD_CN /* 유효기간 */
	      , A.VLD_PRD_DSC  /*유효기간구분코드*/
	      , FN_CM_SIMP_CNM('VLD_PRD_DSC', A.VLD_PRD_DSC) AS VLD_PRD_DSC_NM  /*유효기간구분명*/
	      , A.BSC_UNT_DSC  /*기본단위구분코드*/
	      , FN_CM_SIMP_CNM('BSC_UNT_DSC', A.BSC_UNT_DSC) AS BSC_UNT_DSC_NM  /*기본단위구분명*/
		  , C.CNTN_QT AS PR_MRK_WHT
		  , C.UNTP_UPR_BASQT  AS PR_MRK_STD_BAS_WHT
		  , C.BAS_PR_UNT_DSC AS PR_MRK_BAS_UNIT
		  , FN_CM_SIMP_CNM('BAS_PR_UNT_DSC', C.BAS_PR_UNT_DSC) AS PR_MRK_BAS_UNIT_NM
	    FROM
	      TB_GD_WRS_CLF B,
	      TB_GD_WRS A,
	      TB_BS_GM_PR_UNIT C
	    WHERE 1=1
	      AND A.NA_WRS_LCLC =B.NA_WRS_LCLC
	      AND A.NA_WRS_MCLC = B.NA_WRS_MCLC
	      AND A.NA_WRS_SCLC = B.NA_WRS_SCLC
	      AND A.NA_WRS_DTCF_C = B.NA_WRS_DTCF_C
	      AND A.NA_WRS_C = C.NA_WRS_C (+)
	      AND A.NA_WRS_C = #{NA_WRS_C}
	</select>

	<select id="retrieveNaWrsPdaaC" parameterType="java.util.Map" resultType="java.util.Map">
		SELECT
			  NA_WRS_PDAA_C
			, PDAA_PROVNM
			, PDAA_CCONM
			, PDATTS_NM
			, PDAA_ZIP
			, PDAA_LVL_C
		FROM
			TB_BS_GC_PDAA
		WHERE 1=1
			AND DEL_YN = '0'
			<if test="searchText != null and searchText.length() > 0">
				<choose>
					<when test="searchMode == 'NAME'.toString()">
						AND PDATTS_NM LIKE '%' || #{searchText} || '%'
					</when>
					<when test="searchMode == 'CODE'.toString()">
						AND NA_WRS_PDAA_C = #{searchText}
					</when>
				</choose>
			</if>
		ORDER BY NA_WRS_PDAA_C
	</select>

<!-- 온라인 취급상품 조회 -->
	<!-- 온라인 취급상품 조회 low count -->
	<select id="selectOnLineModyCount" parameterType="java.util.Map" resultType="int">
		<include refid="common.count_start" />
		<include refid="sql_selectOnLine"/>
		<include refid="common.count_end" />
	</select>

	<!-- 온라인 취급상품 조회 -->
	<select id="selectOnLineMody" parameterType="java.util.Map" resultType="java.util.Map">
		<bind name="sortName" value="'FIR_RG_DTM'" />
		<bind name="sortType" value="'asc'" />

		<include refid="common.paging_start" />
		<include refid="sql_selectOnLine"/>
		<include refid="common.paging_end" />
	</select>

	<sql id="sql_selectOnLine">
		SELECT O.NA_WRS_C
				, W.WRSNM
				, O.PDM_MFTCO
				, O.ORGPL_NATCD
				, O.PD_RGN
				, DECODE(O.AGLVPD_MFCPD_YN, '0', '여', '부') AGLVPD_MFCPD_YN
				, O.MTR_AGLVPD_ORG_ORGPL
				, O.TOT_CAP
				, O.UNT_CAP
				, C.SIMP_CNM WRS_UNT_C
				, DECODE(O.ADLT_ATTC_REQUIRED_YN, '0', '여', '부') ADLT_ATTC_REQUIRED_YN
				, TO_CHAR(O.FIR_RG_DTM, 'YYYY-MM-DD HH24:MI:SS') FIR_RG_DTM
				, TO_CHAR(O.LS_CHG_DTM, 'YYYY-MM-DD HH24:MI:SS') LS_CHG_DTM
		FROM  TB_GD_WRS_ONLINE O,
			TB_GD_WRS W,
			PT_USR_INF INF,
			(SELECT B.C_DTL_C AS SIMP_C ,
			  B.DTL_C_NM      AS SIMP_CNM ,
			  B.DTL_C_SNM     AS SIMP_C_EXPL
			  FROM TB_AM_CM_CODE A ,
			    TB_AM_CM_CODE_DTL B
			  WHERE A.C_TP_C = B.C_TP_C
			  AND A.C_TP_C   = 'GD0017'
			) C
		WHERE  O.NA_WRS_C = W.NA_WRS_C
			AND O.WRS_UNT_C = C.SIMP_C (+)
			AND O.FIR_RGMN_ID = INF.MB_ID
			<if test="TRPL != null">
				AND INF.NA_TRPL_C IN
				<foreach collection="TRPL" item="item" index="index" separator="," open="(" close=")">
					#{item}
				</foreach>
			</if>
			<if test="NA_WRS_C != null and NA_WRS_C.length() > 0">
				AND O.NA_WRS_C LIKE #{NA_WRS_C} || '%'
			</if>
			<if test="FROM_DATE != null and FROM_DATE.length() >0">
				<if test="DATE_SEL == 1">
					AND TO_CHAR(O.FIR_RG_DTM) BETWEEN TO_DATE(#{FROM_DATE},'YYYYMMDD') AND TO_DATE(#{TO_DATE} ,'YYYYMMDD')
				</if>
				<if test="DATE_SEL == 2">
					AND TO_CHAR(O.LS_CHG_DTM) BETWEEN TO_DATE(#{FROM_DATE},'YYYYMMDD') AND TO_DATE(#{TO_DATE} ,'YYYYMMDD')
				</if>
			</if>
	</sql>
	
	<select id="downloadExcelGoodWhList" parameterType="java.util.Map"  resultType="java.util.Map" fetchSize="1000">
		<include refid="sql_goodWhList" />
    </select>
    
    <!-- 덤 기준상품 조회 -->
    <select id="retrieveFdGoodDum" parameterType="java.util.Map" resultType="java.util.Map">
		SELECT B.NA_BZPLC
			 , (SELECT CLNTNM FROM TB_TR_TRPL_CIF WHERE NA_TRPL_C = B.NA_BZPLC AND DEL_YN='0' ) NA_BZPLC_NM
			 , FN_CM_SIMP_CNM('DUM_DSC', B.DUM_DSC) DUM_DSC
			 , FN_CM_SIMP_CNM('DUM_EVT_DSC', B.DUM_EVT_DSC) UM_EVT_DSC
			 , FN_CM_SIMP_CNM('QSLPR_TPC', B.PRGR_C) PRGR_C
			 , B.NA_WRS_C
			 , FN_OD_TRTWRS(A.SPYPL_NA_TRPL_C,B.NA_BZPLC, B.NA_WRS_C) NA_WRS_NM
			 , B.DUM_QT
			 , B.BAS_WRS_QT
			 , B.DUM_RTO
			 , B.APL_DT
			 , B.ED_DT
			 , DECODE(B.LMT_YN, 0, '부', '여') LMT_YN
			 , B.BAS_NA_WRS_C
			 , FN_OD_TRTWRS(A.SPYPL_NA_TRPL_C,B.NA_BZPLC, B.BAS_NA_WRS_C) BAS_NA_WRS_NM
			 , B.BAS_NA_WRS_C2
			 , FN_OD_TRTWRS(A.SPYPL_NA_TRPL_C,B.NA_BZPLC, B.BAS_NA_WRS_C2) BAS_NA_WRS_NM2
			 , B.BAS_NA_WRS_C3
			 , FN_OD_TRTWRS(A.SPYPL_NA_TRPL_C,B.NA_BZPLC, B.BAS_NA_WRS_C3) BAS_NA_WRS_NM3
			 , B.BAS_NA_WRS_C4
			 , FN_OD_TRTWRS(A.SPYPL_NA_TRPL_C,B.NA_BZPLC, B.BAS_NA_WRS_C2) BAS_NA_WRS_NM4
			 , B.BAS_NA_WRS_C5
			 , FN_OD_TRTWRS(A.SPYPL_NA_TRPL_C,B.NA_BZPLC, B.BAS_NA_WRS_C3) BAS_NA_WRS_NM5
			 , B.BAS_NA_WRS_C6
			 , FN_OD_TRTWRS(A.SPYPL_NA_TRPL_C,B.NA_BZPLC, B.BAS_NA_WRS_C2) BAS_NA_WRS_NM6
			 , B.BAS_NA_WRS_C7
			 , FN_OD_TRTWRS(A.SPYPL_NA_TRPL_C,B.NA_BZPLC, B.BAS_NA_WRS_C3) BAS_NA_WRS_NM7
			 , B.BAS_NA_WRS_C8
			 , FN_OD_TRTWRS(A.SPYPL_NA_TRPL_C,B.NA_BZPLC, B.BAS_NA_WRS_C2) BAS_NA_WRS_NM8
			 , TO_CHAR(B.FSRG_DTM, 'YYYYMMDD') FSRG_DTM
			 , TO_CHAR(B.LSCHG_DTM, 'YYYYMMDD') LSCHG_DTM
			 , TO_CHAR(B.DEL_DTM, 'YYYYMMDD') DEL_DTM
			 , DECODE(B.DEL_DTM, NULL, '부', '여') DEL_YN
		  FROM TB_OD_TRTWRS A
		     , TB_FD_GM_GOOD_DUM B
	    WHERE A.NA_BZPLC = B.NA_BZPLC
		  AND A.NA_WRS_C = B.NA_WRS_C
		  AND B.DUM_DSC = '1'
	      AND B.PRGR_C = '00'
	      AND A.SPYPL_NA_TRPL_C IN
	      <foreach collection="TRPL_C" item="item" index="index" separator="," open="(" close=")">
				#{item}
		  </foreach>
		  AND B.APL_DT BETWEEN #{FROM_DATE} AND #{TO_DATE}
		  <if test="NA_BZPLC != null and NA_BZPLC.length() > 0">
		  		AND B.NA_BZPLC = #{NA_BZPLC}
		  </if>
		  <if test="NA_WRS_C != null and NA_WRS_C.length() > 0">
		  		AND B.NA_WRS_C = #{NA_WRS_C}
		  </if>
		  <if test="NA_WRS1_C != null and NA_WRS1_C.length() > 0">
		  		AND B.BAS_NA_WRS_C = #{NA_WRS1_C}
		  </if>
		  <if test="DELYN != 'all'">
		  		DECODE(B.DEL_DTM, NULL, '2', '1') = #{CONFIRM}
		  </if>
		ORDER BY B.APL_DT DESC, B.NA_BZPLC DESC  
	</select>
</mapper>