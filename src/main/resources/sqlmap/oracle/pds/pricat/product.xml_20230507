	<sql id="sql_goodWhList">	
          SELECT    ROWNUM      AS RN
                  , A.* FROM
          (
	          SELECT   A.NA_BZPLC
	                 , B.SHRT_BZPLNM      AS NA_BZPLC_NM
	                 , A.NA_WRS_C
	                 , C.WRSNM
	                 , A.NA_SPYPL_C
	                 , D.CLNTNM           AS NA_SPYPL_C_NM
	                 , NVL(A.ATL_YN, '0') AS ATL_YN
	                 , A.PHD_FCLT_C
	                 , A.SRTR_PRC_YN
	                 , A.BOXPE_AQZQT
	                 , NVL(A.BOX_WRS_C_YN, '0') AS BOX_WRS_C_YN
	                 , NVL(A.SVS_YN, '0') AS SVS_YN
	                 , A.SVS_QT
	                 , TO_CHAR(A.DEL_DTM, 'YYYY-MM-DD') DEL_DT

	            FROM   TB_FD_GM_GOOD_WH A
	                 , TB_CM_BZPL B
	                 , TB_GD_WRS C
	                 , TB_TR_TRPL_CIF D
	                 , TB_TR_BZPL_TP E
	           WHERE A.NA_BZPLC   = B.NA_BZPLC
	             AND A.NA_WRS_C   = C.NA_WRS_C
	             AND A.NA_SPYPL_C = D.NA_TRPL_C
	             AND B.BZPL_TPC     = E.BZPL_TPC
	             AND B.BZPL_DTI_TPC = E.BZPL_DTI_TPC
	             AND E.BZPL_TPC IN ( '504' , '505')
	             AND B.DEL_DTM IS NULL
	             AND A.NA_SPYPL_C = #{TRPL_C}
	             AND NVL(A.ATL_YN, '0') = #{ATL_YN}
	             AND NVL(A.BOX_WRS_C_YN, '0') = #{BOX_WRS_C_YN}
	             AND NVL(A.SVS_YN, '0') = #{SVS_YN}




            <if test="NA_WRS_C != null and NA_WRS_C.length() > 0">
                AND A.NA_WRS_C LIKE #{NA_WRS_C} || '%'
            </if>

            <if test="NA_BZPLC != null and NA_BZPLC.length() > 0">
                AND A.NA_BZPLC = #{NA_BZPLC}
            </if>

			<if test="DEL_YN != null and DEL_YN.length() >0">

				<if test="DEL_YN == 0">
	                AND A.DEL_DTM IS NULL
				</if>

				<if test="DEL_YN == 1">
                    AND A.DEL_DTM IS NOT NULL
				</if>

			</if>

	             AND ROWNUM <![CDATA[< ]]> 10000

	        ORDER BY   A.NA_BZPLC
	                 , A.NA_WRS_C
	                 , A.NA_SPYPL_C
             ) A
	</sql>
	
