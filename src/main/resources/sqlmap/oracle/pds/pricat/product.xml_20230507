	<sql id="sql_selectNewGoods">
		SELECT
			A.VAN_C_RQ_NO
			,A.NA_C_RQ_NO
			,A.NA_C_RQ_OBJ_DSC
			,FN_CM_SIMP_CNM('NA_C_RQ_OBJ_DSC', A.NA_C_RQ_OBJ_DSC) AS NA_C_RQ_OBJ_DSC_NM
			,A.CTCPL_TELNO
			,A.RQ_CNTN
			,A.APV_RQ_NA_BZPLC
			,A.FSRGMN_ENO
			,A.FSRGMN_NA_BZPLC
			,A.BUYER_ENO
			,A.RQ_NA_WRS_C
			,A.NA_WRS_C
			,A.WRS_C_DSC
			,A.WRS_DSC
			,A.WRSNM
			,A.WRS_ABR_NM
			,A.WRS_ENM
			,A.WRS_TPC
			,A.NA_WRS_LCLC
			,A.NA_WRS_MCLC
			,A.NA_WRS_SCLC
			,A.NA_WRS_DTCF_C
			,A.WRS_STDNM
			,A.TXT_DSC
			,A.SMA_APL_TPC
			,A.LIQ_YN
			,A.COMM_WRS_YN
			,A.MFT_NA_TRPL_C
			,A.WHT
			,A.NA_WRS_UNT_C
			,A.NA_WRS_PAK_C
			,A.NA_WRS_BRAN_C
			,A.NA_WRS_CSTD_METC
			,A.NA_WRS_DSTR_STSC
			,A.NA_WRS_UZC
			,A.NA_WRS_IPO_DSC
			,A.VCBT_NA_WRS_C
			,A.VCBT_QT
			,A.VCBX_NA_WRS_C
			,A.WRS_DTL_EXPL
			,A.BSC_UNT_DSC
			,A.BOXPE_AQZ
			,A.RFID_UYN
			,A.FOMNM
			,A.SLGT_ETR_DSC
			,A.DSTR_TER_YN
			,A.DSTR_TER_MRK_DSC
			,A.WRS_MODIFY_YN
			,A.TEMP_YN
			,A.PRC_YN
			,A.RSP_C_CNTN
			,A.REQ_RESULT
			,A.CNF_ENO
			,A.CNF_DMT
			,A.CNF_NAME
			,A.NA_C_RQ_REJ_RSNC
			,A.NA_C_RQ_STSC
			, '' NA_WRS_SZE_SEX_C			/*  경제통합상품크기성별코드 */
			, '' NA_WRS_QLT_GRDC			/*  경제통합상품품질등급코드 */
			, '' NA_WRS_PROENV_DSC			/*  경제통합상품친환경구분코드 */
			, '' GAP_ATTC_YN				/*  GAP인증여부 */
			, '' NA_WRS_NACF_QLT_ATTC_C		/*  경제통합상품농협품질인증코드 */
			, '' NA_WRS_PDAA_C				/*  경제통합상품산지코드 */
			, '' ACHMI_LATCNM   			/*  농약품목명 */
			, '' ACHMI_IGRQT    			/*  농약성분량 */
			, '' FTLZ_PHAC_IGRDRT      		/*  비료인산성분비 */
			, '' FTLZ_NTRG_IGRDRT      		/*  비료질소성분비 */
			, '' FTLZ_KLM_IGRDRT			/*  비료칼륨성분비 */
			, '' FMACH_PATS_CLF_DSC    		/*  농기계부품분류구분코드 */
			, '' NUMN_YN        			/*  본체여부 */
			, '' GEN_LON_LMT_AM 			/*  일반융자한도금액 */
			, '' COMM_LON_LMT_AM			/*  공동융자한도금액 */
			, '' NA_WRS_STD_UNT_C      		/*  경제통합상품규격단위코드 */
			, '' WRS_HGHT       			/*  상품높이 */
			, '0' WRS_LEN_LEN    			/*  상품세로길이 */
			, '0' WRS_WDTH_LEN   			/*  상품가로길이 */
			, '0' WRS_SGRT       			/*  상품비중률 */
			, '0' WRS_SDL_DSC    			/*  상품외쌍겹구분코드 */
			, '' WRS_AHL_CN     			/*  상품유공개수 */
			, '' MTRL_STD_THKN  			/*  자재규격두께 */
			, '' CICL_WRS_STD_DMTR_CN  		/*  원형상품규격지름수 */
			, '' CICL_WRS_STD_DPTH_CN  		/*  원형상품규격깊이수 */
			, '' FED_PD_METC    			/*  사료생산방식코드 */
			, '' SNUM_CNTN      			/*  찾기번호내용 */
			, '' CVS_QT						/*  환산수량 */
			, '' CVS_NA_WRS_UNT_C 			/*  환산경제통합상품단위코드 */
			, '' WRS_FL_CN					/*  상품파일개수 */
			, '' KNET_WRS_NO				/*  코리안넷상품번호 */
			, '' TR_STOP_YN     			/*  거래중지여부 */
			, '' TR_STOP_RSNC   			/*  거래중지사유코드 */
			, '' FMACH_FOMNM 				/*  농기계형식명 */
			, NVL(B.BOXPE_DLVG_UPR,'0') BOXPE_DLVG_UPR
			, NVL(B.SLAM_PER_FEERT,'0') SLAM_PER_FEERT
			, NVL(B.PMTN_SPT_PERS,'0') PMTN_SPT_PERS
			, NVL(B.WRS_TRT_CMBR_CN,'0') WRS_TRT_CMBR_CN
			, NVL(B.ASP_SL_CNAM,'0') ASP_SL_CNAM
			, B.DEL_YN
		FROM
			TB_GD_WRS_RG_REQ A, 			/* 상품등록신청내역 */
			TB_GD_WRS_BS_CM_RG_RQ B			/* 등록신청기본 */
		WHERE
			A.NA_C_RQ_NO = B.NA_C_RQ_NO(+)
			AND A.APV_RQ_NA_BZPLC IN
			<foreach collection="TRPL_C" item="item" index="index" separator="," open="(" close=")">
	    		#{item}
			</foreach>
			AND CUD_DSC != 'D'
	</sql>

	<sql id="sql_selectNewGoodsExcel">
		SELECT
			DISTINCT
			A.TEMP_SEQ AS NA_C_RQ_NO
			, '' NA_C_RQ_OBJ_DSC
			, '' CTCPL_TELNO
			, '' RC_DTM
			, '' RCST_ENO
			, '' RCST_NA_BZPLC
			, '' PRC_DTM
			, '' PRCMN_ENO
			, '' PRCMN_NA_BZPLC
			, '' NA_C_RQ_REJ_RSNC
			, '' RQ_CNTN
			, '' PRC_CNTN
			, '' APV_RQ_NA_BZPLC
			, '' DEL_YN
			, '' FSRG_DTM
			, '' FSRGMN_ENO
			, '' FSRGMN_NA_BZPLC
			, '' LSCHG_DTM
			, '' LS_CMENO
			, '' LSCGMN_NA_BZPLC
			, '' OSLF_WRS_RQ_RSNC
			, '' BUYER_RC_DTM
			, '' BUYER_RCST_ENO
			, '' BUYER_RCST_NA_BZPLC
			, '' BUYER_PRC_DTM
			, '' BUYER_PRCMN_ENO
			, '' BUYER_PRCMN_NA_BZPLC
			, '' PU_DLVG_UPR
			, '' BOXPE_DLVG_UPR
			, '' SLAM_PER_FEERT
			, '' PMTN_SPT_PERS
			, '' WRS_TRT_CMBR_CN
			, '' ASP_SL_CNAM
			, '' TFR_DEL_WRS_C
			, '' ROTS_WRS_CNF_YN
			, '' DLVG_CLNTNM
			, '' DLVG_TRPL_TELNO
		FROM TB_GD_WRS_RG_REQ_TEMP A
		WHERE
			1=1
			<if test="center_code != null and center_code.length() > 0">
				AND A.NA_BZPLC = #{center_code}
			</if>
			<if test="NA_WRS_RADIO == 1">
				<if test="NA_WRS_C != null and NA_WRS_C.length() > 0">
					AND A.NA_WRS_C LIKE #{NA_WRS_C} || '%'
				</if>
			</if>
			<if test="NA_WRS_RADIO == 2">
				<if test="NA_WRS_C != null and NA_WRS_C.length() > 0">
					AND A.WRSNM LIKE #{NA_WRS_C} || '%'
				</if>
			</if>
			AND A.FSRGMN_NA_BZPLC IN
			<foreach collection="TRPL_C" item="item" index="index" separator="," open="(" close=")">
	    		#{item}
			</foreach>
			AND TO_CHAR(FSRG_DTM,'YYYYMMDD') <![CDATA[>=]]> #{start_date}
			AND TO_CHAR(FSRG_DTM,'YYYYMMDD') <![CDATA[<=]]> #{end_date}
	</sql>

