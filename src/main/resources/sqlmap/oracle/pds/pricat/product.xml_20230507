	  		, AA.SPYPL_NA_TRPL_C
	  		, AA.CLNTNM
	  		, AA.NA_WRS_C
	  		, AA.WRSNM
		FROM (
			SELECT
	    		A.SPYPL_NA_TRPL_C
	    		, B.CLNTNM
	    		, A.NA_WRS_C
	    		, A.WRSNM
			FROM
