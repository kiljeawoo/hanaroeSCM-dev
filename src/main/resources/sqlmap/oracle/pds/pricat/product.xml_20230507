	<sql id="sql_selectTrtwrs">
		SELECT
		    A.NA_BZPLC,
		    B.CLNTNM,
		    (SELECT TEAMNM FROM TB_CM_TEAM WHERE NA_BZPLC = A.NA_BZPLC
		    AND NA_TEAM_C = A.NA_TEAM_C
  			) AS TEAM_NM,
		    A.NA_WRS_C,
		    A.PD_YY,
		    A.CSER_CTR_TPC,
		    A.BUY_TXT_DSC,
		    A.BUY_BZ_METC,
		    A.NA_TEAM_C,
		    A.WRSNM,
		    A.WRS_ABR_NM,
		    A.NA_WRS_LCLC,
		    A.NA_WRS_MCLC,
		    A.NA_WRS_SCLC,
		    A.NA_WRS_DTCF_C,
		    A.SL_UPR_TPC,
		    A.INPD_QT,
		    A.MIN_ODR_QT,
		    A.MAX_ODR_QT,
			CASE A.BYNG_UPR
                   WHEN 0 THEN '0.00'
                   ELSE TO_CHAR(A.BYNG_UPR, 'fm9999999.99')
               END AS BYNG_UPR,
		    A.SL_UPR,
		    A.VCBT_NA_WRS_C,
		    A.VCBT_UPR,
		    A.VCBX_NA_WRS_C,
		    A.VCBX_UPR,
		    A.SPYPL_NA_TRPL_C,
		    (SELECT CLNTNM
			FROM TB_TR_TRPL_CIF WHERE NA_TRPL_C = A.SPYPL_NA_TRPL_C
			) AS SPYPL_NA_TRPL_NM,
			A.SPYPL_NA_TEAM_C,
			NVL((SELECT DISTINCT TEAMNM FROM TB_CM_TEAM
			WHERE NA_BZPLC = A.SPYPL_NA_TRPL_C AND NA_TEAM_C = A.SPYPL_NA_TEAM_C),'팀없음'
			) AS SPYPL_NA_TEAM_NM,
			A.MNGT_NA_TRPL_C,
			(SELECT CLNTNM
			FROM TB_TR_TRPL_CIF WHERE NA_TRPL_C = A.MNGT_NA_TRPL_C
			) AS MNGT_NA_TRPL_NM,
			A.MNGT_NA_TEAM_C,
			NVL((SELECT DISTINCT TEAMNM FROM TB_CM_TEAM
			WHERE NA_BZPLC = A.MNGT_NA_TRPL_C AND NA_TEAM_C = A.MNGT_NA_TEAM_C),'팀없음'
			) AS MNGT_NA_TEAM_NM,
		    A.CTR_TPC,
		    CASE WHEN A.CTR_TPC IS NOT NULL THEN (FN_CM_SIMP_CNM('CTR_TPC',A.CTR_TPC)) ELSE '' END AS CTR_TPC_NM,
		    A.SPY_TPC,
		    A.TXT_DSC,
		    CASE WHEN A.TXT_DSC IS NOT NULL THEN (FN_CM_SIMP_CNM('TXT_DSC',A.TXT_DSC)) ELSE '' END AS TXT_DSC_NM,
		    A.RVO_PSB_YN,
		    A.ODR_PSB_YN,
		    A.DDLY_YN,
		    A.STPL_PD_VIAPD_DSC,
		    A.STPL_PD_GRD_DSC,
		    A.VIAPD_YN,
		    A.SVS_YN,
		    A.SVS_UNT_QT,
		    A.BOX_BULK,
		    A.WRS_WDTH_LEN,
		    A.WRS_LEN_LEN,
		    A.BOX_WRS_YN,
		    A.DDLY_ST_DT,
		    A.DDLY_ED_DT,
		    A.DDLY_BAS_QT,
		    A.MART_DIV_BKG_ODR_YN,
		    A.SRA_WRS_DSC,
		    A.STPL_AMN_WRS_YN,
		    A.STPL_NA_WRS_C,
		    A.STPL_AMN_DSC,
		    A.PD_YY_AMN_YN,
		    A.NA_ODR_FBID_RSNC,
		    FN_CM_SIMP_CNM('NA_ODR_FBID_RSNC', A.NA_ODR_FBID_RSNC) AS NA_ODR_FBID_RSNC_NM,
		    A.NA_RVO_FBID_RSNC,
        	FN_CM_SIMP_CNM('NA_RVO_FBID_RSNC', A.NA_RVO_FBID_RSNC) AS NA_RVO_FBID_RSNC_NM,
		    A.BOXPE_AQZ,
		    A.NA_USR_SYS_KDC,
		    TO_CHAR(A.FSRG_DTM,'YYYY-MM-DD') AS FSRG_DTM,
		    A.FSRGMN_ENO,
		    TO_CHAR(A.LSCHG_DTM,'YYYY-MM-DD') AS LSCHG_DTM,
		    A.LS_CMENO,
		    TO_CHAR(A.DEL_DTM,'YYYY-MM-DD') AS DEL_DTM
		FROM TB_OD_TRTWRS A,TB_TR_TRPL_CIF B
		<where>
    		B.NA_TRPL_C(+) = A.NA_BZPLC
    		<if test="NA_BZPLC != null and NA_BZPLC.length() > 0">
    			AND A.NA_BZPLC = #{NA_BZPLC}
    			AND A.NA_TEAM_C = #{NA_BZPLC_TEAM}
    		</if>
    		AND A.SPYPL_NA_TRPL_C IN
			<foreach collection="TRPL_C" item="item" index="index" separator="," open="(" close=")">
	    		#{item}
			</foreach>
			<if test="NA_WRS_C != null and NA_WRS_C.length() > 0">
				AND A.NA_WRS_C LIKE #{NA_WRS_C} || '%'
			</if>

			<if test="CTR_TPC != 'all'">
				AND A.CTR_TPC = #{CTR_TPC}
			</if>
			<!-- <if test="CSER_CTR_TPC != 'all'">
				AND A.CSER_CTR_TPC #{CSER_CTR_TPC}
			</if> -->
			<if test="FROM_DATE != null and FROM_DATE.length() >0">
				<if test="DATE_SEL == 1">
					AND A.FSRG_DTM <![CDATA[>=]]> TO_DATE(#{FROM_DATE},'YYYYMMDD')
        			AND A.FSRG_DTM <![CDATA[<=]]> TO_DATE(#{TO_DATE} ,'YYYYMMDD')
				</if>
				<if test="DATE_SEL == 2">
					AND A.LSCHG_DTM <![CDATA[>=]]> TO_DATE(#{FROM_DATE},'YYYYMMDD')
        			AND A.LSCHG_DTM <![CDATA[<=]]> TO_DATE(#{TO_DATE} ,'YYYYMMDD')
				</if>
				<if test="DATE_SEL == 3">
					AND A.DEL_DTM <![CDATA[>=]]> TO_DATE(#{FROM_DATE},'YYYYMMDD')
        			AND A.DEL_DTM <![CDATA[<=]]> TO_DATE(#{TO_DATE} ,'YYYYMMDD')
				</if>
			</if>
			ORDER BY A.NA_BZPLC ASC
		</where>
	</sql>

